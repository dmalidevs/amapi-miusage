	"license": "ISC",
	"dependencies": {
		"@wordpress/block-editor": "^12.25.0",
		"@wordpress/blocks": "^12.34.0",
		"@wordpress/editor": "^13.35.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-query": "^3.39.3"
	}
}