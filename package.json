{
	"name": "hasan-miusage",
	"version": "1.0.0",
	"description": "",
	"main": "vite.config.js",
	"scripts": {
		"vdev": "vite --input-type=module",
		"wpbuild": "wp-scripts build --output-path=assets/block",
		"wpstart": "wp-scripts start --output-path=assets/block",
		"devx": "concurrently \"vite\" \" npm run wpstart\" && npm run build",
		"dev": "concurrently \"vite\" \" npm run wpstart\"",
		"build": "concurrently \"vite build\" \" npm run wpbuild\"",
		"watch": "concurrently \"vite build\" \" npm run wpbuild\"",
		"dev-build": "vite build",
		"preview": "vite preview",
		"zip": "npm run build && node bundle.mjs",
		"zip-full": "vite build && node bundle-full.mjs"
	},
	"devDependencies": {
		"@vitejs/plugin-react": "^4.3.1",
		"@wordpress/scripts": "^19.2.4",
		"concurrently": "^8.2.2",
		"dir-archiver": "^2.1.0",
		"sass": "^1.72.0",
		"vite": "^5.1.6",
		"vite-plugin-zip-pack": "^1.2.3"
	},
	"keywords": [],
